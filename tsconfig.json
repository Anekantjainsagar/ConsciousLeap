{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["app/*"]
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/therapists/[name]/page.jsx"
, "app/user/dashboard/page.jsx", "app/user/dashboard/edit/page.jsx", "app/(main)/therapists/page.jsx", "app/(main)/user/dashboard/Components/LeftBar.jsx", "app/(main)/user/dashboard/Components/LeftBar copy.jsx", "app/(main)/questionnaire/questions/ask-age/page.jsx", "app/(main)/questionnaire/page.jsx"  ],
  "exclude": ["node_modules"]
}
